{
  "name": "whats-for-dinner",
  "version": "0.4.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-oauth/google": "^0.5.1",
    "ariakit": "2.0.0-next.41",
    "axios": "^1.6.8",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development pnpm generate:css --watch",
    "dev:start": "react-scripts start",
    "build": "run-s build:*",
    "build:css": "cross-env NODE_ENV=production pnpm generate:css --minify",
    "build:prod": "react-scripts build",
    "serve": "pnpx serve -s build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test:coverage": "cross-env CI=true pnpm test -- --env=jsdom --coverage",
    "generate:css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "pnpm exec tsc -b",
    "validate": "run-s format lint typecheck && pnpm test -- --watchAll=false",
    "clean": "pnpx rimraf pnpm-lock.yaml node_modules .cache styles/tailwind.css",
    "tailwind:viewer": "tailwind-config-viewer -o",
    "postinstall": "pnpm generate:css"
  },
  "prettier": {},
  "eslintIgnore": [
    "node_modules",
    "dist",
    "build",
    "public",
    "src/index.css"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.12",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/gapi": "^0.0.43",
    "@types/gapi.client.drive": "^3.0.15",
    "@types/google.accounts": "^0.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.31",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwind-config-viewer": "^1.7.3",
    "tailwindcss": "^3.4.3"
  }
}
